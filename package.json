{
  "name": "testing-hooks",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "live-server ./src",
    "format": "prettier --log-level warn --write \"src/**/*.{js,css,md}\"",
    "posttest": "npm run format",
    "lint": "eslint \"src/**/*.js\"",
    "pretest": "npm run lint",
    "prepare": "husky"
  },
  "lint-staged": {
    "src/**/*.js": [ "eslint --fix", "prettier --write" ],
    "src/**/*.css": [ "prettier --write"]  
  },
  "keywords": [],
