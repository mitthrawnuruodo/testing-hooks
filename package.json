  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "live-server": "^1.2.2",
    "mrm": "^4.1.22",
    "mrm-task-eslint": "^4.1.22",
    "mrm-task-lint-staged": "^7.1.22",
    "mrm-task-prettier": "^5.1.22",
    "prettier": "^3.3.3"
  }
